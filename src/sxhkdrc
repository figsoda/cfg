button8
    xdotool set_desktop $(( (`xdotool get_desktop` + 1) % `xdotool get_num_desktops` ))

button9
    xdotool set_desktop $(( (`xdotool get_desktop` + `xdotool get_num_desktops` - 1) % `xdotool get_num_desktops` ))

